<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LottoBoard">
        <attr name="background_color" format="color|reference" />
        <attr name="border_size" format="dimension|reference" />
        <attr name="border_color" format="color|reference" />
        <attr name="divider_size" format="dimension|reference" />
        <attr name="horizontal_field_count" format="integer|reference" />
        <attr name="vertical_field_count" format="integer|reference" />
        <attr name="text_color" format="color|reference"/>
        <attr name="text_size" format="dimension|reference"/>
        <attr name="corner_radius" format="dimension|reference"/>
        <attr name="corner_type" format="enum">
            <enum name="sharp" value="0" />
            <enum name="round" value="1" />
        </attr>
    </declare-styleable>

</resources>